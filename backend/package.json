{
  "name": "bkeep-backend",
  "version": "1.0.0",
  "description": "...",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prestart": "pnpm run build",
    "start": "node -r dotenv/config dist/server.js",
    "dev": "nodemon",
    "worker:mail": "node -r dotenv/config dist/workers/mail.worker.js",
    "worker:mail:dev": "nodemon --watch src --exec ts-node -r tsconfig-paths/register src/workers/mail.worker.ts",
    "build": "pnpm run clean && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate:prod": "knex migrate:latest --knexfile dist/config/knexfile.js --env production",
    "db:migrate:rollback:prod": "knex migrate:rollback --knexfile dist/config/knexfile.js --env production",
    "db:migrate:list:prod": "knex migrate:list --knexfile dist/config/knexfile.js --env production",
    "db:seed:prod": "knex seed:run --knexfile dist/config/knexfile.js --env production",
    "db:migrate": "knex migrate:latest --knexfile src/config/knexfile.ts",
    "db:migrate:make": "knex migrate:make --migrations-directory src/database/migrations -x ts",
    "db:migrate:tenant:make": "knex migrate:make --migrations-directory src/database/migrations/tenant -x ts",
    "db:migrate:rollback": "knex migrate:rollback --knexfile src/config/knexfile.ts",
    "db:migrate:rollback:all": "knex migrate:rollback --knexfile src/config/knexfile.ts --all",
    "db:migrate:list": "knex migrate:list --knexfile src/config/knexfile.ts",
    "db:seed": "knex seed:run --knexfile src/config/knexfile.ts",
    "db:seed:make": "knex seed:make --knexfile src/config/knexfile.ts -x ts",
    "docker:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml up --build -d",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.933.0",
    "@simplewebauthn/server": "^13.2.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.28.2",
    "connect-session-knex": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.10",
    "objection": "^3.1.5",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.6",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/express-rate-limit": "^6.0.2",
    "@types/express-session": "^1.18.2",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.0",
    "@types/node-cache": "^4.2.5",
    "@types/nodemailer": "^7.0.3",
    "@types/pg": "^8.15.6",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  }
}
